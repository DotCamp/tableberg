{
    "name": "dotcamp/tableberg",
    "description": "Tableberg plugin by Dotcamp.",
    "require": {
        "dotcamp/promoter": "^1.0"
    },
    "require-dev": {
        "php-stubs/wordpress-stubs": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Tableberg\\": [
                "./",
                "includes/"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "vendor/dotcamp/promoter"
        }
    ]
}
