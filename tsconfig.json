{
	"$schema": "https://json.schemastore.org/tsconfig.json",
    "compilerOptions": {
        "outDir": "./dist",
        "target": "ES2015",
        "jsx": "react-jsx",
        "module": "ESNext",
        "strict": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "lib": [
            "es2019",
            "dom"
        ]
    }
}

