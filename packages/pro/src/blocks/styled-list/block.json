{
     "$schema": "https://schemas.wp.org/trunk/block.json",
     "apiVersion": 3,
     "name": "tableberg/styled-list",
     "title": "Styled List",
     "category": "design",
     "description": "This block lets you add a styled list in your post or pages. Instead of bullets you can use various icons as your list style type. ",
     "keywords": [
          "List",
          "Styled List",
          "tableberg"
     ],
     "parent": [
          "tableberg/cell",
          "tableberg/styled-list-item"
     ],
     "textdomain": "tableberg-pro",
     "attributes": {
          "isOrdered": {
               "type": "boolean",
               "default": false
          },
          "alignment": {
               "type": "string",
               "default": "left"
          },
          "itemSpacing": {
               "type": "number",
               "default": 1
          },
          "listSpacing": {
               "type": "number",
               "default": 5
          },
          "iconColor": {
               "type": "string",
               "default": "#000000"
          },
          "iconSize": {
               "type": "number",
               "default": 10
          },
          "iconSpacing": {
               "type": "number",
               "default": 2
          },
          "fontSize": {
               "type": "string",
               "default": ""
          },
          "textColor": {
               "type": "string",
               "default": ""
          },
          "backgroundColor": {
               "type": "string",
               "default": ""
          },
          "padding": {
               "type": "object",
               "default": {}
          },
          "margin": {
               "type": "object",
               "default": {}
          },
          "listStyle": {
               "type": "string",
               "default": "disc"
          },
          "icon": {
               "type": "object",
               "default": null
          },
          "parentCount" : {
               "type": "number",
               "default": 0
          }
     },
     "supports": {},
     "editorScript": "tableberg-pro-block-script",
     "editorStyle": "tableberg-pro-editor-style",
     "style": "tableberg-pro-frontend-style"
}